#!/usr/bin/env python

import re

marked = {}
prev = {}
def extract_links(path):
    f = open(path)
    text = f.read()
    pat = re.compile('href=[\'"]?([^\'" >]+)')
    links = re.findall(pat, text)
    return links

def visited(path): 
    try:
        return marked[path]
    except(Exception):
        return False
        
def BFS(src, dest):
    q = [src]
    while len(q) > 0:
        s = q[0]
        if s == dest:
            return s
        links = filter(visited, extract_links(s))
        q.append(links)
        
        
